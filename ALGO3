def below_arithmetical_mean(arr):
    arithmetical_mean = sum(arr) / len(arr)
    result_list = []
    for i in arr:
        if i < arithmetical_mean:
            result_list.append(i)
    return result_list


test_list = [1,3,5,6,10,2,3]
result = below_arithmetical_mean(test_list)
print(result)


def two_lowest_elements(arr):
    arr.sort()
    return arr[:2]


test_list1 = [1,3,5,6,4,10,2,3]
result = two_lowest_elements(test_list1)
print(result)
